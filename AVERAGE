#include<stdio.h>

int marks(int m1, int m2, int m3) {
    if (m1 >= 40 && m2 >= 40 && m3 >= 40) {
        return 1;
    } else {
        return 0;
    }
}

int main() {
    int m1, m2, m3;
    float avg;

    printf("Enter the marks of m1, m2 and m3:\n");
    scanf("%d %d %d", &m1, &m2, &m3);

    avg = (m1 + m2 + m3) / 3.0;

    printf("The average marks is %.2f\n", avg);

    if (marks(m1, m2, m3)) {
        printf("RESULT: PASS\n");
    } else {
        printf("RESULT: FAIL\n");
    }

    return 0;
}
