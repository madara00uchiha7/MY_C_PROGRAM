// Reads an integer and float value
#include<stdio.h>
int main()
{
    int a;
    float b;
    printf("Enter an integer\n");
    scanf("%d",&a);
    printf("Enter a float value\n");
    scanf("%f",&b);
    printf("\nEntered integer value is%d",a);
    printf("\nEntered float value is%.2f",b);
    return 0;
}
