#include<stdio.h>
#include<string.h>
int main(){
    char string[100];
    char str[100];
    printf("Enter the name\n");
    fgets(string,sizeof(string),stdin);
    printf("Enter the second name\n");
    fgets(str,sizeof(str),stdin);
    int a=strcmp(string,str);
   printf("ACCORDING TO ASCII COMPAIRISON IS  %d\n",a);
}
