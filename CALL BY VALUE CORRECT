#include<stdio.h>
int value(int x,int y){
    int temp;
    temp=x;
    x=y;
    y=temp;
    printf("x=%d and y=%d\n", x,y);
    
}
int main(){
    int a,b;
    printf("Enter the number for a and b\n");
    scanf("%d%d",&a,&b);
    value(a,b);
    printf("The number after swapping %d and %d",a,b);
}
