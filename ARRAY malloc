#include<stdio.h>
#include<stdlib.h>
int main(){
    int i,n;
    int *ptr;
    printf("Enter the value for n\n");
    scanf("%d",&n);
    ptr=(int*)malloc(sizeof(int));
     printf("Enter %d elements:\n", n);
     for(i=0;i<n;i++){
        scanf("%d",&ptr[i]);
        }
        printf("Entered elements is:");
        for(i=0;i<n;i++){
        printf("%d\t",ptr[i]);
      
        }
          free(ptr);
}
