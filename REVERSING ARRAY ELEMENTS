#include<stdio.h>
int main(){
    int arr[50],n,i;
    printf("Enter the value of n\n");
    scanf("%d",&n);
    printf("Enter elements for n\n");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("Print Reversed array\n");
    for(i=n-1;i>=0;i--){
        printf("%d",arr[i]);
    }
}    
        
    
    
