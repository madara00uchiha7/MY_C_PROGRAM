include <stdio.h>

int main() {
    int n, i,fact=1;
    int a[100];          
    printf("Enter a number: ");
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        a[i] = i + 1;
    }
    for(i = 0; i < n; i++) {
        fact = fact * a[i];
    }
    printf("Factorial of %d = %d\n", n, fact);
}

